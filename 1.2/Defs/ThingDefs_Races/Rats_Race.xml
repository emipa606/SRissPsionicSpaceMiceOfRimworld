            <li>Male</li>
            <li>Female</li>
          </alienbodytypes>
          <useGenderedHeads>true</useGenderedHeads>
          <bodyAddons>
            <li>
              <path>Things/Rats/Tail/RTail</path>
              <bodyPart>tail</bodyPart>
              <inFrontOfBody>false</inFrontOfBody>
              <drawnInBed>false</drawnInBed>
              <drawSize>0.67</drawSize>
              <colorChannel>skin</colorChannel>
              <offsets>
                <south>
                  <bodyTypes>
                    <Male>(-0.37,0.21)</Male>
                    <Female>(-0.36,0.21)</Female>
                    <Thin>(-0.41,0.21)</Thin>
                  </bodyTypes>
                  <crownTypes>
